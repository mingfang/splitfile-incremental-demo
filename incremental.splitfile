FROM ${DESTINATION}

SQL {
    CREATE TABLE IF NOT EXISTS ${TABLE}
    AS TABLE "${SOURCE}".${TABLE}
    WITH NO DATA
}
SQL {
    ALTER TABLE ${TABLE} DROP CONSTRAINT IF EXISTS ${TABLE}_pkey;
    ALTER TABLE ${TABLE} ADD CONSTRAINT ${TABLE}_pkey PRIMARY KEY (${KEY});
}

${EXTRA_SQL}

SQL{
    INSERT INTO ${TABLE} (
        SELECT * FROM "${SOURCE}".${TABLE}
    )
}